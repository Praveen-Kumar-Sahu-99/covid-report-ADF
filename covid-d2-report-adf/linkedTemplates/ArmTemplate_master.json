{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid-d2-report-adf"
		},
		"ls_ablob_covidreportd2sa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covidreportd2sa'"
		},
		"ls_adls_covidreportdd2dl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covidreportdd2dl'"
		},
		"ls_adls_covidreportdd2dl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreportd2dl.dfs.core.windows.net/"
		},
		"ls_http_ecdc_data_death_cases_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com"
		},
		"tr_ingest_hospital_admission_data_properties_pl_ecdc_deaths_data_parameters_sourceRelativeURL": {
			"type": "string",
			"defaultValue": "cloudboxacademy/covid19/blob/main/ecdc_data/hospital_admissions.csv"
		},
		"tr_ingest_hospital_admission_data_properties_pl_ecdc_deaths_data_parameters_sinkFileName": {
			"type": "string",
			"defaultValue": "hospital_admissions.csv"
		},
		"tr_ingest_population_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/c779884c-8177-4bd2-8251-157a109c2430/resourceGroups/covid-report-rg/providers/Microsoft.Storage/storageAccounts/covidreportd2sa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid-d2-report-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covidreportd2sa_connectionString": {
						"value": "[parameters('ls_ablob_covidreportd2sa_connectionString')]"
					},
					"ls_adls_covidreportdd2dl_accountKey": {
						"value": "[parameters('ls_adls_covidreportdd2dl_accountKey')]"
					},
					"ls_adls_covidreportdd2dl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covidreportdd2dl_properties_typeProperties_url')]"
					},
					"ls_http_ecdc_data_death_cases_properties_typeProperties_url": {
						"value": "[parameters('ls_http_ecdc_data_death_cases_properties_typeProperties_url')]"
					},
					"tr_ingest_hospital_admission_data_properties_pl_ecdc_deaths_data_parameters_sourceRelativeURL": {
						"value": "[parameters('tr_ingest_hospital_admission_data_properties_pl_ecdc_deaths_data_parameters_sourceRelativeURL')]"
					},
					"tr_ingest_hospital_admission_data_properties_pl_ecdc_deaths_data_parameters_sinkFileName": {
						"value": "[parameters('tr_ingest_hospital_admission_data_properties_pl_ecdc_deaths_data_parameters_sinkFileName')]"
					},
					"tr_ingest_population_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_ingest_population_data_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}